@model List<IGrouping<string, CinephilesChoice.Models.Nomination>>

@{
    ViewData["Title"] = "Navigate To Awards";
    Layout = "~/Views/Shared/_Layout_Cinephile.cshtml";
}

<h1>NavigateToAwards</h1>

<div>
    <ul>
        @for(int i = 0; i < Model.Count; i++)
        {
            <button id='"year" + i.ToString()' onclick="displayAwardCategories('awards' + i.ToString())">@Model[i].Key</button>
            <ul id='"awards" + i.ToString()' style="display:none;">
                @for(int j = 0; j < Model[i].ToList().Count; j++)
                {
                    <li>@Model[i].ToList()[j].AwardCategory</li>
                }
            </ul>
        }
    </ul>
</div>
