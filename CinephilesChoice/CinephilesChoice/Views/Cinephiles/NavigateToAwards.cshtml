@model List<IGrouping<string,CinephilesChoice.Models.Nomination>>

@{
    ViewData["Title"] = "Navigate To Awards";
    Layout = "~/Views/Shared/_Layout_Cinephile.cshtml";
}

<h1>NavigateToAwards</h1>

<div>
    <ul>
        @for(int i = 0; i < Model.Count; i++)
        {
            <li id='"year" + i.ToString()'>@Model[i].Key</li>
        }
    </ul>
</div>
